<!-- Twitter -->
<meta name="twitter:card" content="4ª Plan de Acción Nacional 2018 - 2020" />
<meta name="twitter:site" content="<%= Link::URL_BASE %>" />
<meta name="twitter:title" content="<%= social_title %>" />
<meta name="twitter:description" content="<%= social_description %>" />
<meta name="twitter:image" content="<%= asset_url('uruguaylogo_200.png') %>" />
<!-- Facebook OG -->
<meta id="ogtitle" property="og:title" content="<%= social_title %>"/>
<% if setting['url'] %>
  <meta property="article:publisher" content="<%= setting['org_name'] %>"/>
<% end %>
<% if setting['facebook_handle'] %>
  <meta property="article:author" content="https://www.facebook.com/<%= setting['facebook_handle'] %>"/>
<% end %>
<meta property="og:type" content="Agesic"/>
<meta id="ogurl" property="og:url" content="<%= Link::URL_BASE %>"/>
<meta property="og:title" content="<%= social_title %>"/>
<meta id="ogimage" property="og:image" content="<%= asset_url('uruguaylogo_200.png') %>"/>
<meta property="og:site_name" content="<%= setting['org_name'] %>"/>
<meta id="ogdescription" property="og:description" content="<%= social_description %>"/>
<meta property="fb:app_id" content="<%= Rails.application.secrets.facebook_key %>"/>
